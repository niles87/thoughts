{
  "name": "deep-thoughts",
  "version": "1.0.0",
  "description": "social network site",
  "main": "server/server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niles87/thoughts.git"
  },
  "keywords": [
    "social"
  ],
  "author": "niles87",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/niles87/thoughts/issues"
  },
  "homepage": "https://github.com/niles87/thoughts#readme",
  "dependencies": {
    "if-env": "^1.0.4"
  },
  "devDependencies": {
    "concurrently": "^6.0.0"
  }
}
